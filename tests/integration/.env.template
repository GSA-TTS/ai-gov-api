# Environment variables for integration tests
# Copy this file to .env and fill in the actual values

# API Configuration
API_BASE_URL=your_live_endpoint_here
TEST_API_KEY=your_test_api_key_here

# Available Models (comma-separated)
CHAT_MODELS=claude_3_5_sonnet,claude_3_7_sonnet,gemini-2.0-flash,llama3_8b
EMBEDDING_MODELS=cohere_english_v3,text-embedding-005

# Test Configuration
TEST_TIMEOUT=30
TEST_MAX_TOKENS=100
TEST_TEMPERATURE=0.1

# Optional: Provider-specific configuration
AWS_REGION=us-east-1
GCP_PROJECT_ID=your-project-id #google project id